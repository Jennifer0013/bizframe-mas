<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>MSH Web Application</display-name>
	<description>Application for BizFrame MSH 3.0</description>
   <filter>                
   	<filter-name>monitoring</filter-name>               
   	<filter-class>net.bull.javamelody.MonitoringFilter</filter-class>      
   </filter>        
   <filter-mapping>              
		<filter-name>monitoring</filter-name>         
		<url-pattern>/*</url-pattern>       
  </filter-mapping>        <listener>        
   <listener-class>net.bull.javamelody.SessionListener</listener-class>        </listener>
	<listener>
		<listener-class>kr.co.bizframe.startup.ContextListener</listener-class>
	</listener>
	
	<!--listener>
		<listener-class>kr.co.bizframe.ecf.web.WebBeanContextListener</listener-class>
	</listener-->
	
		
	<servlet>
		<servlet-name>MxsReceiver</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.MxsReceiver</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MxsSendRequestReceiver</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.DefaultSendRequestReceiver</servlet-class>
	</servlet>
	<!-- ANT_REPLACE_ConsumerQueueServer_START -->
	<servlet>
		<servlet-name>ConsumerQueueServer</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.ConsumerQueueServer</servlet-class>
	</servlet>
	<!-- ANT_REPLACE_CHANNEL_SERVER_END -->
	<servlet>
		<servlet-name>MxsJFServlet</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.FileReceiverServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MSIRemoteReceiver</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.MSIRemoteReceiver</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SessionServer</servlet-name>
		<servlet-class>kr.co.bizframe.mxs.transport.http.SessionServer</servlet-class>
	</servlet>
	<!--servlet>
		<servlet-name>EbRemoteReceiver</servlet-name>
		<servlet-class>kr.co.bizframe.ecf.ha.ebms.EbRemoteReceiver</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet-->
	
	
	
	<!--servlet-mapping>
		<servlet-name>EbRemoteReceiver</servlet-name>
		<url-pattern>/ecf_j</url-pattern>
	</servlet-mapping-->
	
	
	<servlet-mapping>
		<servlet-name>MxsReceiver</servlet-name>
		<url-pattern>/msh</url-pattern>
	</servlet-mapping>
	<!-- ANT_REPLACE_CHANNEL_SERVER_START -->
	<servlet-mapping>
		<servlet-name>ConsumerQueueServer</servlet-name>
		<url-pattern>/queue</url-pattern>
	</servlet-mapping>
	<!-- ANT_REPLACE_CHANNEL_SERVER_END -->
	<servlet-mapping>
		<servlet-name>MxsSendRequestReceiver</servlet-name>
		<url-pattern>/msi_j</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MxsJFServlet</servlet-name>
		<url-pattern>/msi_jf</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MSIRemoteReceiver</servlet-name>
		<url-pattern>/msi_ji</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SessionServer</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<exception-type>java.lang.Exception</exception-type>
		<location>/err00zz500.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/err00zz404.jsp</location>
	</error-page>
</web-app>